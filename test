let chars = [1, 3, 4, 4,2,2,9,11,12];
let newArray = [];
for (let i = 0; i < chars.length; i++) {
  if (!newArray.length) {
    newArray.push(chars[i]);
  } else {
    let isPushAble = 1;
    for (let j = 0; j < newArray.length; j++) {
      if (newArray[j] == chars[i]) {
        isPushAble = -1;
        break;
      }
    }
    if (isPushAble>-1) {
      newArray.push(chars[i]);
    }
  }
}
console.log("index", newArray);
